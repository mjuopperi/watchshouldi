{
  "firebase": "watchshouldi",
  "public": ".",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "**/docs/**"
  ],
  "rewrites": [
    { "source": "**/vote/*", "destination": "/vote.html" },
    { "source": "**/vote.css", "destination": "/css/vote.css" },
    { "source": "**/vote.js", "destination": "/js/vote.js" },
    { "source": "**/util.js", "destination": "/js/util.js" },
    { "source": "**/lib/lodash.js", "destination": "/js/lib/lodash.js" },
    { "source": "**/lib/jquery.inlinesvg.js", "destination": "/js/lib/jquery.inlinesvg.js" },
    { "source": "**/preloader.gif", "destination": "/img/preloader.gif" },
    { "source": "**/default_poster.svg", "destination": "/img/default_poster.svg" },
	{ "source": "**/lib/autosize.min.js", "destination": "/js/lib/autosize.min.js" },
	{ "source": "**/lib/ZeroClipboard.js", "destination": "/js/lib/ZeroClipboard.js" },
	{ "source": "**/lib/ZeroClipboard.swf", "destination": "/js/lib/ZeroClipboard.swf" },
	{ "source": "**/copy.svg", "destination": "/img/copy.svg" }
  ]
}
